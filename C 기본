printf : 입력한 값 출력scanf : 앞으로 입력할 값 출력int : 정수변수선언char : 문자변수 선언a=b : b를 a에 대입http://web.yonsei.ac.kr/hgjung/Lectures/Gen131/index.html#include<stdio.h>==============================================================복합대입연산자#include<stdio.h>void main(){ int s1, s2; s1=s2=5; printf("s1+1=%d\n",s1+=1); printf("s1-1=%d\n",s1-=1); printf("s1*s2=%d\n",s1*=s2); printf("s1/1=%d\n",s1/=s2); printf("s1%%1=%d\n",s1%=s2);}================================================================증감연산자전치연산           후치연산a++,a--             ++a,--a=================================================================관계연산자<   ~보다작다>   ~보다크다<= ~보다 작거나 같다>= ~보다 크거나 같다== 같다!=  같지않다================================================================논리 연산자&&                     ex)여자&&직장인                   남학생인경우:    0           0             0        여학생인경우:    1           0             0 남자직장인인 경우:    0           1             0 여자직장인인 경우:    1           1             1||                        ex)여자||직장인        남학생인경우:    0           0             0        여학생인경우:    1           0             1 남자직장인인 경우:    0           1             1 여자직장인인 경우:    1           1             1=================================================================#include<stdio.h>void main(void){ printf("논리합(참 || 참) : %d\n",10>0||10<50); printf("논리합(참 || 거짓) : %d\n",10<50||10<0); printf("논리합(거짓 || 참) : %d\n",10>50||10<50); printf("논리합(거짓 || 거짓) : %d\n",10>50||10<0); printf("논리곱(참 && 참) : %d\n",10>0&&10<50); printf("논리곱(참 && 거짓) : %d\n",10<50&&10<0); printf("논리곱(거짓 && 참) : %d\n",10>50&&10<50); printf("논리곱(거짓 && 거짓) : %d\n",10>50&&10<0); printf("부정(!참) : %d\n",!(10>0)); printf("부정(!거짓) : %d\n",!(10>100));}==================================================================#include<stdio.h>void main(void){int a=10, b=4 ,res;a>0 ? printf("참인 경우 실행 \n"):printf("거짓인경우 실행\n");a%2==0 ? printf("짝수 \n"):printf("홀수 \n");res=a>b ? a:b;printf("두개의 수 중 큰수 : %d\n",res); }==================================================================#include<stdio.h>void main(void){ int num1, num2, sum; printf("정수2개를 입력하세요 :"); scanf("%d %d",&num1, &num2); sum = num1+num2; printf("두 수의 합은 %d입니다.\n",sum);}===================================================================#include<stdio.h>                       //이름 나이 혈액형 알기int main(void){ char 이름[30]; int 나이; char 혈액형[30]; float 키; printf("이름, 나이, 혈액형, 키를 입력해주세요 :"); scanf(" %s %d %s %f",&이름, &나이, &혈액형, &키); printf("이름 : %s\n 나이 : %d세\n 혈액형 : %s형\n 키 : %fcm\n",이름,나이,혈액형,키);}                                                     ==================================================================== #include<stdio.h>             5의배수계산기void main(void){ int num; scanf("%d",&num); if(num%5==0){  printf("5의 배수입니다.");} else{  printf("5의 배수가 아닙니다.");}}                              ======================================================================#include<stdio.h>                     1~99숫자 알기void main(void){ int num; scanf("%d",&num); if(0>num&&num<100){  printf("정상");} else{  printf("비정상"); }}=========================================================================#include<stdio.h>                                                          논리연산자,if문 활용void main(void){ int num; scanf("%d",&num); if(num%3==0&&num%2==0){  printf("%d\n",num);}}======================================================================if else 문          배수계산기#include<stdio.h>void main(void){ int num; printf("정수를 하나 입력해주세요 :"); scanf("%d",&num); if(num%2==0&&num%3==0&&num%5==0)  printf("2와 3과 5의 배수");else if(num%5==0&&num%2==0){        printf("2와5의 배수\n");}else if(num%5==0&&num%3==0){        printf("3과5의 배수\n");}else if(num%2==0&&num%3==0){        printf("2와3의 배수\n");}    else if(num%2==0){  printf("2의 배수\n");}else if(num%3==0){        printf("3의 배수\n");}else if(num%5==0){        printf("5의 배수\n");}else if(num%5==0&&num%2==0){        printf("2와5의 배수\n");}else if(num%5==0&&num%3==0){        printf("3과5의 배수\n");}else if(num%2==0&&num%3==0){        printf("2와3의 배수\n");}else{ printf("해당사항없음\n");}}====================================================================switch case문#include<stdio.h>void main(void){ int select; scanf("%d", &select); switch(select){ case 1 : printf("1입력\n");  break; case 2 : printf("2입력\n");  break; case 3 : printf("3입력\n");  break; case 4 : printf("4입력\n");  break; default : printf("기타입력\n");  break; }}break : if문을 제외한 나머지 제어문을 빠져나옴=================================================================계산기 case활용#include<stdio.h>void main(void){ int a, b; char ff; printf("정수 두개를 입력하세요:"); scanf("%d %d", &a, &b); fflush(stdin); printf("\n연산기호를 입력하세요:(+,-,*,/)"); scanf("%c",&ff); switch(ff) { case '+' : printf("%d+%d=%d\n",a,b,a+b);  break; case '-' : printf("%d-%d=%d\n",a,b,a-b);  break; case '*' : printf("%d*%d=%d\n",a,b,a*b);  break; case '/' : printf("%d/%d=%d\n",a,b,a/b);  break; default : printf("오류");  }}===================================================================while문 중괄호 안의 조건이 거짓이 될때까지 반복#include<stdio.h>void main(void){int n=10;while(n<12){ printf("현재 n 값 : %d \n",n);}}출력값 :현재 n값 : 10 반복======================================================================#include<stdio.h>               while문 10 11 12 출력void main(void){int n=10;while(1)//1=참이다.         {   printf("현재 n값 : %d\n",n);  if(n==12){  printf("if문으로 while빠져나가기 \n");   break; } n++;//10부터 1씩더해서 12가 될때까지 while로 돌아간다     }}===========================================================================#include <stdio.h> void   main( ){ int n = 1; int sum=0; while(n<101){  printf("%d\n",n);  n++; }}===================================================================#include <stdio.h> void   main( ){ int n = 1; int sum=0; while(n<11){  sum += n; // sum = sum +n; n++; } printf("%d\n",sum);} ====================================================================== #include <stdio.h> void   main( ){ int n = 1; scanf("%d",&n);  while(n>0){ printf("%d",n%10); n/=10;                           //n = n/10; }}==========================================================================#include <stdio.h> void   main( ){ int n; while(1){  printf("숫자를 입력하세요 : 가위(0)/바위(1)/보(2)/종료(3)\n");  scanf("%d",&n);  switch(n){  case 0 : printf("가위가 선택됨 \n"); break;  case 1 : printf("바위가 선택됨 \n"); break;  case 2 : printf("보가 선택됨 \n"); break;  case 3 : printf("게임을 종료합니다. \n"); return;  default : printf("잘못된입력입니다. \n");   } }}============================================================================== do while#include <stdio.h> void   main( ){ int n; do {  printf("1.가위/2.바위/3.보/4.종료\n");  printf("메뉴선택: ");  scanf("%d",&n);  printf("%d번 선택됨 \n\n",n); }while(n!=4); printf("프로그램종료\n");}==================================================================================for문 : 괄호 안에 식이 3개#include <stdio.h> void   main( ){ int i;                                                    //i 같은 변수를 반복수라 함 (묵시적으로 i부터 씀) for(i = 0; i<2; i++) {  printf("i=%d\n",i); }}===================================================================================#include <stdio.h>                                                      0~10까지 출력하는 for문 void   main( ){ int i; for(i=0; i<11; i++){  printf("%d\n",i); \ }}=======================================================================================#include<stdio.h>void main(){ int i; for(i=1;i<=4;i++) {  printf("%d.Hello\n",i); }}=========================================================================#include<stdio.h>void main(){ //65~90 char i; for(i=65;i<90;i++){  printf("%c,",i); } printf("%c",i++);}같다.#include<stdio.h>void main(){ //65~90 char i; for(i='A';i<='Z';i++){ if(i=='Z'){  printf("%c\n",i);} else{ printf("%c,",i); }} }===========================================================================for(;;)=무한루프=====================================================================#include<stdio.h>void main(){ int a,b;  for(a=0; a<2; a++){   printf("외부for문을 실행 : %d 번째 \n",a);   for(b=0; b<3; b++){    printf("***내부 for문 실행 : %d번째 \n", b);   }   printf("\n\n");   }  }========================================================================상황구분 : 기중상황 및 반복상황 설정2. 기준 상황은 외부 for문으로 작성반복상황은 내부 for문으로 작성3.for문의 새부적인 작성은 단일 for문과 같음======================================================================#include<stdio.h>           구구단void main(){ int a,b;  for(a=1; a<=9; a++){   printf("",a);   for(b=1; b<=9; b++){    printf("%d*%d=%d\n",a,b,a*b);   }   printf("\n");   }  }=======================================================================#include<stdio.h>               가로 구구단void main(){ int a,b; for(a=1;a<=9;a++){  printf("",a);  for(b=1;b<=9;b++){   printf("|%d*%d=%d|",a,b,a*b);  }  printf("\n"); }}========================================================================#include<stdio.h> 가로구구단2           void main(){ int a,b;  for(a=1; a<=9; a++){   printf("",a);   for(b=1; b<=9; b++){    printf("|%d * %d = %2d|",b,a,b*a);   }   printf("\n");   }}========================================================================break문반복문 또는 switch_case문에서 현재 진행중인 제어문의 흐름을 빠져나가는 기능continue문반복문 내부에서 현재 위치 이후의 내용을 무시하고 반복문의 처음으로 돌아가는 기능return문현재 위치한 함수의 이후 기능을 무시하고 함수를 종류하는 기능exit(인자값)프로그램을 종료하는 기능인자갑 : 0-정상종료/0을 제외한 수 - 비정상종료#include<windows.h>====================================================================#include<stdio.h>#include<windows.h>        //exit(0);void main(){ int num; while(1){  printf("입장객 인원을 입력하세요(1~3명)(종료 0):");  scanf("%d",&num);  if(num==0){   printf("프로그램 종료\n");   exit(0);//#include<windows.h>가 있어야지 가능  }else if(num<0||num>3){   printf("잘못된 입력입니다.\n\n");   continue;  }  printf("입장객 인원 : %d \n",num); }}=========================================================================배열램에 공간생성.선언한후 초기화 안됨=======================================================================#include<stdio.h>void main(){ int n1[3]={1,2,3}; int n2[2]; n2[0] = 4; n2[1] = 5; printf("n1 첫번째 데이터 : %d \n", n1[0]); printf("n1 두번째 데이터 : %d \n", n1[1]); printf("n1 세번째 데이터 : %d \n", n1[2]); printf("n2 첫번째 데이터 : %d \n", n1[0]); printf("n2 두번째 데이터 : %d \n", n1[1]); }======================================================================#include<stdio.h>void main(){ int n[5]={1,2,3,4,5};  printf("전체 데이터 출력 ? : %d \n", n); printf("배열 주소 출력 !! =>%d \n",&n);  }결론 :1.배열이름은 배열의 시작주소를 의미한다.2.각 공간의 값에 접근할때에는 배열의 이름만 불러서는 안된다.#include<stdio.h>void main(){ int n1[4]={1,2,3,4}; int n2[4];  n1[1]=5; n1[2]=6; n1[3]=7; n1[4]=8; printf("전체 데이터 출력 ? : %d \n", n); printf("배열 주소 출력 !! =>%d \n",&n);  }====================================================================#include<stdio.h>void main(){ int a[4]={1,2,3,4};//{}=초기화 int b[4];  b[0]=10; b[1]=20; b[2]=30; b[3]=40; printf("%d,%d,%d,%d\n",a[0],a[1],a[2],a[3]); printf("%d,%d,%d,%d\n",b[0],b[1],b[2],b[3]);  }================================================================#include<stdio.h>void main(){ int a[4]={1,2,3,4};//{}=초기화 int b[4]; int i;  b[0]=10; b[1]=20; b[2]=30; b[3]=40; for(i=0;i<4;i++)  printf("%d\n",a[i]); for(i=0;i<4;i++)  printf("%d\n",b[i]);                               //배열은 for와 궁합 좋음  }====================================================================1. 배열이름==배열 전체의 시작주소==배열[0] 공간의 주소2.배열의 낱개 공간 주소 접근 방법=>&배열[공간번호]ex) &ar[3]===================================================================#include<stdio.h>void main(){ int a[3]; printf("배열 a 전체주소(시작주소) : %d \n",a); printf("배열 a[0] 의 주소: %d \n",&a[0]); printf("배열 a[1] 의 주소: %d \n",&a[1]); printf("배열 a[2] 의 주소: %d \n",&a[2]);  }결론 : 배열의 주소값은 자료형의 크기만큼 차이가 남int:4char:1double:8======================================================================#include<stdio.h>void main(){ int a[3]; printf("배열 a 전체주소(시작주소) : %d \n",a); printf("배열 a[0] 의 주소: %d \n",&a[0]); printf("배열 a[1] 의 주소: %d \n",&a[1]); printf("배열 a[2] 의 주소: %d \n",&a[2]); scanf("%d",&a[0]); printf("%d",a[0]);  }=====================================================================#include<stdio.h>void main(){ double a[3]={1,2,3}; double b[3];   printf("배열 a 전체주소(시작주소) : %d \n",a); printf("배열 a[0] 의 주소: %d \n",&a[0]); printf("배열 a[1] 의 주소: %d \n",&a[1]); printf("배열 a[2] 의 주소: %d \n",&a[2]); scanf("%d",&b[0]); printf("%d",b[0]);  }===================================================================배열에 정수 10개를 입력받아 첨자가 짝수인 경우만 출력하시오int a[10]일경우 a[0]a[2]a[4]a[6]a[8]짝수 첨자 배열이 가진 값만 출력이 되게 만드시오#include<stdio.h>void main(){ int a[10]; int i;  for(i=0;i<10;i++){  printf("입력:");     scanf("%d",&a[i]); }  for(i=0;i<10;i++){   if(i%2==0){    printf("%d\n",a[i]);   }  }}====================================================================선택 정렬오름차순 정렬:작은수부터 큰수 또는 앞단어 부터 뒷단어로 감내림차순 정렬:오름차순정렬에 반대0번방에 기준 1234방 비교 제일 작은수 0번방으로1번방 기준 234방 비교 제일 작은수 1번방으로문 : 난수 5개를 입력하여 선택정렬 오름차순으로 정리하라.====================================================================#include <stdio.h>                                       오름차순,내림차순 알고리즘void  main( ){  int arr[]={7,4,9,5,1}; int i,j,tmp; for(i=0;i<4;i++){  for(j=i+1;j<5;j++){   if(arr[i]>arr[j]){    tmp = arr[i];    arr[i] = arr[j];    arr[j] = tmp;   }  } } printf("오름차순 정렬:"); for(i=0;i<5;i++)  printf("%-5d",arr[i]); for(i=0;i<4;i++){  for(j=i+1;j<5;j++){   if(arr[i]<arr[j]){    tmp = arr[i];    arr[i] = arr[j];    arr[j] = tmp;   }  } } printf("\n내림차순 정렬:"); for(i=0;i<5;i++)  printf("%-5d",arr[i]);}==========================================================#include<stdio.h>                                           오름차순정렬void main(){ int arr[]={7,4,9,5,1}; int i,j,tmp;  for(i=0;i<4;i++){  for(j=i+1;j<5;j++)//하나씩 커지니까 j=i+1  {   if(arr[i]>arr[j]){    tmp=arr[i];    arr[i]=arr[j];    arr[j]=tmp;   }           }    } printf("오름차순 정렬 :"); for(i=0;i<5;i++)  printf("%-5d",arr[i]);}===================================================================#include<stdio.h>                                           등수구하기 알고리즘void main(){ int arr[5]={82,85,76,79,96}; int rank[5]; int i,j; for(i=0;i<5;i++){  rank[i]=1;  for(j=0;j<5;j++){    if(arr[i]<arr[j]){    rank[i]++;    }  } } for(i=0;i<5;i++)  printf("%d|%d\n",arr[i],rank[i]);}======================================================================strcpy():문자열을 복사해주는 함수strncpy():지정된 크기 만큼만 문자열을 복사해주는 함수=====================================================================#include <stdio.h>#include <stdlib.h>#include <time.h>void   main( ){ char comsel[3][20]={  "가위",  "바위",  "보" }; int sel; int com;  srand(time(NULL)); while(1){  printf("가위(0)/바위(1)/보(2)/종료(3) : ");  scanf("%d",&sel);  com = rand()%3;  switch(sel){   case 0: printf("가위 선택 / %s선택\n",comsel[com]);    if(sel!=com){     if(com==1){      printf("당신은 패배자\n");     }else{      printf("축하드립니다.!!!!\n");     }    }else{     printf("비기셨습니다..\n");        }    break;   case 1: printf("바위 선택 / %s선택\n",comsel[com]);    if(sel!=com){     if(com==2){      printf("당신은 패배자\n");     }else{      printf("축하드립니다.!!!!\n");     }    }else{     printf("비기셨습니다..\n");        }    break;   case 2: printf("보 선택 / %s선택\n",comsel[com]);    if(sel!=com){     if(com==0){      printf("당신은 패배자\n");     }else{      printf("축하드립니다.!!!!\n");     }    }else{     printf("비기셨습니다..\n");        }     break;   case 3:printf("게임을 종료\n");     exit(0);   default :printf("잘못된 입력입니다.\n");  }  system("pause");  system("cls"); }}=====================================================================#include<stdio.h>      //1초마다 숫자#include<windows.h>void main(){ int num=0; while(1){  printf("%d\n",num);  num++;  Sleep(1000); }}===================================================================#include <stdio.h>#include<windows.h>void main(){ int num=0; while(1){  printf("%d\n",num);  num++;  Sleep(1000);  if(num % 10 == 0){   system("cls");  } }}==================================================================함수 정의하는 이유ex) 약사1명=100    약사 10명 = 10==============================================#include<stdio.h>void str();//함수선언void main(){//함수호출 str();}void str(){ int age=10;//함수정의 printf("나의 나이는 %d살 입니다.\n",age);}=========================================#include<stdio.h>void str();//함수선언int aa=1516; //전역변수:전체영역 사용가능,프로그램이 끝나면 메모리에서 사라짐void main(){//함수호출(지역변수) str(10);}void str(int c){//함주정의(지역변수), int c가 있어서 위에있는 str(10)이 가능 printf("str=%d\n",c); }============================================================#