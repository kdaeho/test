===================================================#include <stdio.h>void main(){ char a= 'A'; double b=12.345; char *pta; double *ptb; pta=&a; ptb=&b; printf("pta의 크기 : :%d\n",sizeof(pta)); printf("ptb의 크기 : :%d\n",sizeof(ptb)); printf("*pta의 크기 : :%d\n",sizeof(*pta)); printf("*ptb의 크기 : :%d\n",sizeof(*ptb)); }//결론 : 1. 포인터변수의 크기는 4바이트//2. 포인터 변수의 자료명은 주소가 가르키는 변수의 자료형을 의미한다.===================================================================#include <stdio.h>void main(){ int a[5]={10,20,30,40,50}; int *pta = a; printf("pta+0=%d\n",*(pta+0)); printf("pta+1=%d\n",*(pta+1)); printf("pta+2=%d\n",*(pta+2)); printf("pta+3=%d\n",*(pta+3)); printf("pta+4=%d\n",*(pta+4));   }//결론 : 1. 배열의 경우, *(pta + 원소번호)또는 pta[원소번호]로 배열의 값에 접근 가능//2. 포인터 변수의 자료명은 주소가 가르키는 변수의 자료형을 의미한다.-----------------------------------------------------------------------------------------------#include<stdio.h>void int_swap(int *x,int*y)void main(){ int a=100,b=200; printf("교환전 : a=%d,b=%d\n",a,b); int_swap(&a,&b); printf("교환후 : a=%d,b=%d\n",a,b);}void int_swap(int *x,int*y){ int tmp; tmp=*x; *x=*y; *y=tmp;}==================================================================#include <stdio.h>void main(){ int a[5]={10,20,30,40,50}; int *pta = a; printf("pta+0=%d %d %d %d\n",*(pta+0),pta[0],a[0],*(a+0));//a=b,b=c printf("pta+1=%d %d %d\n",*(pta+1),pta[1],a[1]); printf("pta+2=%d %d\n",*(pta+2),pta[2]); printf("pta+3=%d %d\n",*(pta+3),pta[3]); printf("pta+4=%d %d\n",*(pta+4),pta[4]);    }//결론 : 1. 배열의 경우, *(pta + 원소번호)또는 pta[원소번호]로 배열의 값에 접근 가능//2. 포인터 변수의 자료명은 주소가 가르키는 변수의 자료형을 의미한다.====================================================================교환#include<stdio.h>void int_swap(int *x);void main(){ int a[2]={100,200}; printf("교환전 : a=%d,b=%d\n",a[0],a[1]); int_swap(a); printf("교환후 : a=%d,b=%d\n",a[0],a[1]);}void int_swap(int *x){ int tmp; tmp=x[0]; x[0]=x[1]; x[1]=tmp;}================================================================#include<stdio.h>void int_swap(int *x,int*y)void main(){ int a=100,b=200;